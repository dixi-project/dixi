{% include 'includesAdmin/header.html' %}
<div id="page-wrapper">
	{% include 'includesAdmin/header2.html' %}
	<div id="page-content-wrapper">
		<div id="page-content">
			<div class="container">
				<div id="page-title">
					<h2>Agregar Registro</h2>
					<p>{{dominio|upper}}</p>
				</div>
				<div class="panel panel-default">
					<div class="panel-body">
						<form id="frm1" name="frm1" action="{{pathCMSSite}}maestro" method="post" enctype="multipart/form-data" class="form-horizontal bordered-row">
							<input type="hidden" name="Dominio" id="txtDominio" value="{{dominio}}"/>
							<input type="hidden" name="idReg" id="idReg" value="{{idReg}}"/>
							<input type="hidden" name="Action" id="Action" value=""/>
							<input type="hidden" name="path" id="path" value="{{pathCMSSite}}"/>
							<div class="form-group">
								<label class="col-sm-2 control-label">
									Maestro
								</label>
								<div class="col-sm-10">
									<input type="text" class="form-control form-control" id="txtMaestro" name="txtMaestro" value="{{datos.maestro}}"/>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">
									Telefono
								</label>
								<div class="col-sm-10">
									<input type="text" class="form-control form-control" id="txtTelefono" name="txtTelefono" value="{{datos.telefono}}"/>
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-2 control-label">
									grupo
								</label>
								<div class="col-sm-10" >
								<select name="cars" id="cars" class="form-control">
								  {% for keyx, value in grupos %}
  								<option value="{{value.id}}">{{value.grupo}}</option>
								  {% endfor %}
								</select>
								</div>
							</div>
							<div class="form-group">
								<br/>
								<div class="col-lg-offset-2 col-lg-10">
									<button type="submit" class="btn btn-sm btn-info" name="cmdRegresar" id="cmdRegresar" data-action="maestro" value="1">
										<i class="glyph-icon icon-angle-double-left"></i>
										Regresar
									</button>
									<button type="submit" class="btn btn-sm btn-success" name="cmdEditar">
										<i class="glyph-icon icon-save"></i>
										Guardar
									</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
{% include 'includesAdmin/footerJS.html' %}
